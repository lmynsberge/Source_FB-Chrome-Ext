{
    "name": "FakeBook",
    "manifest_version": 2,
    "version": "0.1",
    "description": "Evaluates news articles on Facebook to see whether or not they are accurate or coming from a biased source.",
    // "icons": { "128": "icon.png" },
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        // "default_icon": "icon.png"
    }
}